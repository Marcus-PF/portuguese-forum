    "rootDir": "src",
    "outDir": "../../../dist/out-tsc/libs/shared/utils",
    "tsBuildInfoFile": "../../../dist/out-tsc/libs/shared/utils/tsconfig.tsbuildinfo",
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
