{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "rootDir": "src",
    "outDir": "../../../dist/out-tsc/libs/shared/ui",
    "tsBuildInfoFile": "../../../dist/out-tsc/libs/shared/ui/tsconfig.tsbuildinfo",
    "jsx": "react-jsx",
    "types": [],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.css"],
  "exclude": ["**/*.spec.ts", "**/*.test.ts", "**/*.js", "node_modules", "dist"],
  "references": [
    { "path": "../utils" },
    { "path": "../config" }
  ]
}
